@article{kasstan_l_2018,
	title = {(l) as a sociolinguistic variable in {Francoprovençal}},
	volume = {2018},
	issn = {0165-2516},
	url = {https://www.degruyter.com/view/j/ijsl.2018.2018.issue-249/ijsl-2017-0039/ijsl-2017-0039.xml?format=INT},
	doi = {10.1515/ijsl-2017-0039},
	abstract = {This article argues for (l) as a sociolinguistic variable in Francoprovençal: (l) refers to variable palatalisation of /l/ in obstruent+lateral onset clusters (/kl, ɡl, pl, bl, fl/), a feature that has long been the subject of metalinguistic commentary, but no systematic analysis. Our data, which come from a larger study of Francoprovençal (FP), show significant intraspeaker variation. Sociolinguistic interviews were carried out in the Lyonnais region of France among 21 FP speakers with different acquisition routes. /l/-palatalisation is far from categorical in our sample, with increased rates of the French variant [l] over the traditional [j] variant. We interpret these data as contact-induced change: phonological leveling is underway, with convergence towards French. These findings are consistent with the language death literature, and are now widely reported in the context of other minority varieties spoken in the Hexagon. Conversely, some new speakers show different patterns with a greater range of palatalised variants. These findings add to a growing body of evidence that suggest laterals to be a locus for socio-indexical cues cross-linguistically.},
	number = {249},
	urldate = {2019-03-17},
	journal = {International Journal of the Sociology of Language},
	author = {Kasstan, Jonathan and Müller, Daniela},
	year = {2018},
	keywords = {Francoprovençal, language obsolescence, new speakers, palatalisation, variation and change},
	pages = {99--118},
	file = {Full Text PDF:/home/bob/Literature/Zotero/storage/PD6SQXWE/Kasstan and Müller - 2018 - (l) as a sociolinguistic variable in Francoprovenç.pdf:application/pdf},
}

