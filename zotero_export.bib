	pages = {515--530},
	file = {PubMed Central Full Text PDF:/home/bob/Literature/Zotero/storage/ELTLPJZI/Green and Abutalebi - 2013 - Language control in bilinguals The adaptive contr.pdf:application/pdf},
}

